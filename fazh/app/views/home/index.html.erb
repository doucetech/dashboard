<div class="head">
  <h1>LOGO</h1>
  <div class="account">
    <% if current_user %>
      Logged in as <%= current_user.email %>.
      <%= link_to 'Log Out', logout_path %>
      <% else %>
      <%= link_to 'Sign Up', signup_path %> or
      <%= link_to 'Log In', login_path %>
    <% end %>
  </div>
</div>
<div class="school-updates">
  <% @home.each do |home| %>
    <h4><%= home.title %></h4>
    <p><%= home.body %></p>
  <% end %>
</div>
